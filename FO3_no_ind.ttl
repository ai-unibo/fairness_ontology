@base <http://fairness_ontology.com/> .

<http://fairness_ontology.com/> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://fairness_ontology.com/hasAntonym
:hasAntonym rdf:type owl:AnnotationProperty ;
            rdfs:comment "Relates a concept to its antonym, given as a string or URI."@en ;
            rdfs:label "has antonym"@en .


###  http://fairness_ontology.com/isBasedOnSocialPrinciple
:isBasedOnSocialPrinciple rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/source
dc:source rdf:type owl:AnnotationProperty .


###  http://semanticscience.org/resource/hasSynonym
resource:hasSynonym rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  https://purl.org/twc/fmo#probabilistic_definition
fmo:probabilistic_definition rdf:type owl:AnnotationProperty .


###  https://purl.org/twc/fmo#tag
fmo:tag rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://fairness_ontology.com/ExposesTo
:ExposesTo rdf:type owl:ObjectProperty ;
           rdfs:domain :FairnessConcern ;
           rdfs:range fmo:bias ;
           rdfs:label "exposes to"@en .


###  http://fairness_ontology.com/Guides
:Guides rdf:type owl:ObjectProperty ;
        owl:inverseOf :isGuidedBy ;
        rdfs:domain :SocialPrinciple ;
        rdfs:range fmo:fairness_notion ;
        rdfs:comment "Links a fairness notion to its underpinning social principle such as equity or equality."@en ;
        rdfs:label "guides"@en .


###  http://fairness_ontology.com/IsAbout
:IsAbout rdf:type owl:ObjectProperty ;
         rdfs:range :AIModel ;
         rdfs:label "is about"@en .


###  http://fairness_ontology.com/IsTrainedOn
:IsTrainedOn rdf:type owl:ObjectProperty ;
             rdfs:domain :AIModel ;
             rdfs:range :TrainingDataset ;
             rdfs:label "is trained on"@en .


###  http://fairness_ontology.com/IsTrainedWith
:IsTrainedWith rdf:type owl:ObjectProperty ;
               rdfs:domain :AIModel ;
               rdfs:range :ExperimentalSetting ;
               rdfs:label "is trained with"@en .


###  http://fairness_ontology.com/defines
:defines rdf:type owl:ObjectProperty ;
         rdfs:domain :ExperimentalSetting ;
         rdfs:range :HyperparameterConfiguration ;
         rdfs:label "defines"@en .


###  http://fairness_ontology.com/enforces
:enforces rdf:type owl:ObjectProperty ;
          owl:inverseOf :isEnforcedBy ;
          rdfs:domain :MitigationTechnique ;
          rdfs:range fmo:fairness_metric ;
          rdfs:label "enforces"@en .


###  http://fairness_ontology.com/extends
:extends rdf:type owl:ObjectProperty ;
         rdfs:domain :ScientificArtifact ;
         rdfs:range :ScientificArtifact ;
         rdfs:label "extends" ;
         skos:definition "A certain Scientific Artifact may extend another. E.g., a certain Fairness Notion can extend another one" .


###  http://fairness_ontology.com/follows
:follows rdf:type owl:ObjectProperty ;
         owl:inverseOf :isFollowedBy ;
         rdfs:label "follows" .


###  http://fairness_ontology.com/hasBeenAppliedTo
:hasBeenAppliedTo rdf:type owl:ObjectProperty ;
                  rdfs:domain :ScientificArtifact ;
                  rdfs:range :ApplicationDomain ;
                  rdfs:label "has been applied to"@en ;
                  skos:definition "A certain fairness notion has been applied to certain application domains."@en .


###  http://fairness_ontology.com/hasBeenTestedOn
:hasBeenTestedOn rdf:type owl:ObjectProperty ;
                 rdfs:domain :MitigationTechnique ;
                 rdfs:range :Dataset ;
                 rdfs:label "has been tested on"@en .


###  http://fairness_ontology.com/isAddressedWith
:isAddressedWith rdf:type owl:ObjectProperty ;
                 rdfs:domain :FairnessConcern ;
                 rdfs:range fmo:fairness_notion ;
                 rdfs:label "is addressed with"@en .


###  http://fairness_ontology.com/isEnforcedBy
:isEnforcedBy rdf:type owl:ObjectProperty ;
              rdfs:domain fmo:fairness_metric ;
              rdfs:range :MitigationTechnique ;
              rdfs:label "is enforced by"@en .


###  http://fairness_ontology.com/isEquivalentTo
:isEquivalentTo rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:label "is equivalent to"@en ;
                skos:definition "A certain fairness notion can be equal to some other fairness notions."@en .


###  http://fairness_ontology.com/isFollowedBy
:isFollowedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :AILifecycleStep ;
              rdfs:range :AILifecycleStep ;
              rdfs:label "is followed by" .


###  http://fairness_ontology.com/isGuidedBy
:isGuidedBy rdf:type owl:ObjectProperty ;
            rdfs:domain fmo:fairness_notion ;
            rdfs:range :SocialPrinciple ;
            rdfs:comment "Links a fairness notion to its underpinning social principle such as equity or equality."@en ;
            rdfs:label "is guided by"@en .


###  http://fairness_ontology.com/isInstanceOf
:isInstanceOf rdf:type owl:ObjectProperty ;
              rdfs:domain :HyperparameterConfiguration ;
              rdfs:range :Hyperparameter ;
              rdfs:label "is instance of"@en .


###  http://fairness_ontology.com/isMeasuredBy
:isMeasuredBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :measures ;
              rdfs:domain fmo:fairness_notion ;
              rdfs:range fmo:fairness_metric ;
              rdfs:label "is measured by"@en ;
              skos:definition "A fairness notion is measured by a fairness metric if comparisons made with the given metric are a valid way to determine fairness with this notion."@en .


###  http://fairness_ontology.com/isMitigatedBy
:isMitigatedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :mitigates ;
               rdfs:domain fmo:bias ;
               rdfs:range fmo:fairness_notion ;
               rdfs:label "is mitigated by"@en ;
               skos:definition "A certain bias in the use-case can be mitigated by enforcing specific fairness notions."@en .


###  http://fairness_ontology.com/isProposedBy
:isProposedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :ScientificArtifact ;
              rdfs:range :ScientificPaper ;
              rdfs:label "is proposed by"@en ;
              skos:definition "A certain fairness notion is proposed by a certain scientific paper."@en .


###  http://fairness_ontology.com/isSuitableFor
:isSuitableFor rdf:type owl:ObjectProperty ;
               rdfs:domain :ScientificArtifact ;
               rdfs:range :MLTask ;
               rdfs:label "is suitable for"@en ;
               skos:definition "A certain fairness notion is suitable for a certain set of tasks."@en .


###  http://fairness_ontology.com/isUsedBy
:isUsedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :ScientificArtifact ;
          rdfs:range :ScientificPaper ;
          rdfs:label "is used by"@en ;
          skos:definition "A certain fairness notion is used by some scientific papers."@en .


###  http://fairness_ontology.com/is_operationalized_by
:is_operationalized_by rdf:type owl:ObjectProperty ;
                       owl:inverseOf :operationalize ;
                       rdfs:domain :Legal_Requirement ;
                       rdfs:range fmo:fairness_notion ;
                       rdfs:label "is operationalized by" .


###  http://fairness_ontology.com/its_compliance_is_supported_by
:its_compliance_is_supported_by rdf:type owl:ObjectProperty ;
                                owl:inverseOf :supports_compliance_with ;
                                rdfs:domain :Legal_Requirement ;
                                rdfs:range :MitigationTechnique ;
                                rdfs:label "its compliance is supported by" .


###  http://fairness_ontology.com/measuredWith
:measuredWith rdf:type owl:ObjectProperty ;
              rdfs:domain :FairnessConcern ;
              rdfs:range fmo:fairness_metric ;
              rdfs:label "measured with"@en .


###  http://fairness_ontology.com/measures
:measures rdf:type owl:ObjectProperty ;
          rdfs:domain fmo:fairness_metric ;
          rdfs:range fmo:fairness_notion ;
          rdfs:label "measures"@en ;
          skos:definition "A fairness metric measures some fairness notion if comparisons made with the given metric are a valid way to determine fairness with this notion."@en .


###  http://fairness_ontology.com/mitigatedWith
:mitigatedWith rdf:type owl:ObjectProperty ;
               rdfs:domain :FairnessConcern ;
               rdfs:range :MitigationTechnique ;
               rdfs:label "mitigated with"@en .


###  http://fairness_ontology.com/mitigates
:mitigates rdf:type owl:ObjectProperty ;
           rdfs:domain fmo:fairness_notion ;
           rdfs:range fmo:bias ;
           rdfs:label "mitigates"@en ;
           skos:definition "A fairness notion is required to mitigate a certain bias."@en .


###  http://fairness_ontology.com/operationalize
:operationalize rdf:type owl:ObjectProperty ;
                rdfs:domain fmo:fairness_notion ;
                rdfs:range :Legal_Requirement ;
                rdfs:label "operationalize" .


###  http://fairness_ontology.com/supports_compliance_with
:supports_compliance_with rdf:type owl:ObjectProperty ;
                          rdfs:domain :MitigationTechnique ;
                          rdfs:range :Legal_Requirement ;
                          rdfs:label "supports compliance with" .


###  http://fairness_ontology.com/testedOn
:testedOn rdf:type owl:ObjectProperty ;
          rdfs:domain :FairnessConcern ;
          rdfs:range :Dataset ;
          rdfs:label "tested on"@en .


###  http://fairness_ontology.com/IsValidatedOn#is_validated_on
<http://fairness_ontology.com/IsValidatedOn#is_validated_on> rdf:type owl:ObjectProperty ;
                                                             rdfs:domain :AIModel ;
                                                             rdfs:range :ValidationDataset ;
                                                             rdfs:label "is validated on" .


###  http://semanticscience.org/resource/SIO_000068
resource:SIO_000068 rdf:type owl:ObjectProperty ;
                    rdfs:domain :Legal_Requirement ;
                    rdfs:range :Legislation ;
                    rdfs:label "is part of"@en .


###  http://semanticscience.org/resource/SIO_000244
resource:SIO_000244 rdf:type owl:ObjectProperty ;
                    rdfs:domain fmo:fairness_metric ;
                    rdfs:range fmo:statistical_metric ;
                    rdfs:label "is derived from"@en .


###  http://semanticscience.org/resource/SIO_000668
resource:SIO_000668 rdf:type owl:ObjectProperty ;
                    rdfs:label "in relation to"@en .



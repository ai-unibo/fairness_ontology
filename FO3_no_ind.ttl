#################################################################
#    Classes
#################################################################

###  http://fairness_ontology.com/AILifecycleStep
:AILifecycleStep rdf:type owl:Class ;
                 rdfs:label "AI Lifecycle Step" .


###  http://fairness_ontology.com/AIModel
:AIModel rdf:type owl:Class ;
         rdfs:label "AI Model"@en ;
         skos:definition "An artificial intelligence model."@en .


###  http://fairness_ontology.com/Adapters
:Adapters rdf:type owl:Class ;
          rdfs:subClassOf :PartialFineTuning ;
          rdfs:label "Adapters" .


###  http://fairness_ontology.com/AdversarialLearning
:AdversarialLearning rdf:type owl:Class ;
                     rdfs:subClassOf :FullRetraining ;
                     rdfs:label "Adversarial Learning" .


###  http://fairness_ontology.com/AlgorithmicBias
:AlgorithmicBias rdf:type owl:Class ;
                 rdfs:subClassOf fmo:bias ;
                 dc:source "https://doi.org/10.1145/3457607"^^xsd:anyURI ;
                 rdfs:label "algorithmic bias"@en ;
                 skos:definition "Algorithmic bias is when the bias is not present in the input data and is added purely by the algorithm."@en .


###  http://fairness_ontology.com/AmortizedGranularityMetric
:AmortizedGranularityMetric rdf:type owl:Class ;
                            rdfs:subClassOf :GranularityMetric ;
                            rdfs:label "Amortized Granularity Metric"@en .


###  http://fairness_ontology.com/ApplicationDomain
:ApplicationDomain rdf:type owl:Class ;
                   rdfs:label "Application Domain"@en ;
                   skos:definition "AI systems are tailored to the application domain of the specific use-case."@en .


###  http://fairness_ontology.com/AuditSets
:AuditSets rdf:type owl:Class ;
           rdfs:subClassOf :DataGeneration ;
           rdfs:label "Audit Sets" .


###  http://fairness_ontology.com/BanditsBasedLearning
:BanditsBasedLearning rdf:type owl:Class ;
                      rdfs:subClassOf :FullRetraining ;
                      rdfs:label "Bandits-based Learning" .


###  http://fairness_ontology.com/BlackBoxDecisionOnly
:BlackBoxDecisionOnly rdf:type owl:Class ;
                      rdfs:subClassOf :ModelAccessResourcesMitigation ;
                      rdfs:label "Decision Layer Control - Black-box Model (I)" .


###  http://fairness_ontology.com/Blinding
:Blinding rdf:type owl:Class ;
          rdfs:subClassOf :FeatureLevelColumnIntervention ;
          rdfs:label "Blinding and Proxy Removal" .


###  http://fairness_ontology.com/Calibration
:Calibration rdf:type owl:Class ;
             rdfs:subClassOf :GreyBoxScores .


###  http://fairness_ontology.com/CalibrationBasedFairnessMetric
:CalibrationBasedFairnessMetric rdf:type owl:Class ;
                                rdfs:subClassOf fmo:fairness_metric ;
                                rdfs:label "Calibration-based Fairness Metric"@en ;
                                skos:definition "A measurement of any fairness notion, partially connected to the confusion matrix principles, these metrics are usually referred with the term  calibration-based, with the very aim of distinguishing their prevalent attention to the predictive values."@en .


###  http://fairness_ontology.com/CausalFairnessNotion
:CausalFairnessNotion rdf:type owl:Class ;
                      rdfs:subClassOf :OutcomeFairnessNotion ;
                      owl:disjointWith :ObservationalFairnessNotion ;
                      resource:hasSynonym "causal fairness"@en ;
                      rdfs:label "causal fairness notion"@en ;
                      skos:definition "A family of fairness notions focusing on the causal relations between variables."@en .


###  http://fairness_ontology.com/ClassifierFairnessMetric
:ClassifierFairnessMetric rdf:type owl:Class ;
                          rdfs:subClassOf :GeneratedTextBasedFairnessMetric ;
                          rdfs:label "Classifier Fairness Metric"@en ;
                          skos:definition "A metric that employs a classifier on generated text to assess the presence of bias or fairness."@en .


###  http://fairness_ontology.com/Cleaning
:Cleaning rdf:type owl:Class ;
          rdfs:subClassOf :InstanceLevelRowIntervention ;
          rdfs:label "Cleaning" .


###  http://fairness_ontology.com/ConfusionMatrixBasedFairnessMetric
:ConfusionMatrixBasedFairnessMetric rdf:type owl:Class ;
                                    rdfs:subClassOf fmo:fairness_metric ;
                                    rdfs:label "Confusion Matrix-based Fairness Metric"@en ;
                                    skos:definition "A measurement of any fairness notion, focusing on the amount of correct/incorrect classifications inferred by the predictor."@en .


###  http://fairness_ontology.com/ConstrainedOptimization
:ConstrainedOptimization rdf:type owl:Class ;
                         rdfs:subClassOf :FullRetraining ;
                         rdfs:label "Constrained Optimization" .


###  http://fairness_ontology.com/CounterfactualAugmentation
:CounterfactualAugmentation rdf:type owl:Class ;
                            rdfs:subClassOf :InstanceLevelRowIntervention ;
                            rdfs:label "Counterfactual Augmentation" .


###  http://fairness_ontology.com/DataGeneration
:DataGeneration rdf:type owl:Class ;
                rdfs:subClassOf :DataResourcesMitigation ;
                rdfs:label "Data Acquisition/Generation (D)" .


###  http://fairness_ontology.com/DataResourcesMitigation
:DataResourcesMitigation rdf:type owl:Class ;
                         rdfs:subClassOf :MitigationTechnique ;
                         rdfs:label "Data Resources Mitigation"@en .


###  http://fairness_ontology.com/Dataset
:Dataset rdf:type owl:Class ;
         rdfs:label "Dataset"@en ;
         skos:definition "A dataset used in machine learning."@en .


###  http://fairness_ontology.com/DecisionTimeConstrainedDecisioning
:DecisionTimeConstrainedDecisioning rdf:type owl:Class ;
                                    rdfs:subClassOf :BlackBoxDecisionOnly ;
                                    rdfs:label "Decision-time Constrained Decisioning" .


###  http://fairness_ontology.com/DistanceBasedFairnessMetric
:DistanceBasedFairnessMetric rdf:type owl:Class ;
                             rdfs:subClassOf fmo:fairness_metric ;
                             rdfs:label "Distance-based Fairness Metric" .


###  http://fairness_ontology.com/DistributionFairnessMetric
:DistributionFairnessMetric rdf:type owl:Class ;
                            rdfs:subClassOf :GeneratedTextBasedFairnessMetric ;
                            rdfs:label "Distribution Fairness Metric"@en ;
                            skos:definition "A metric that evaluates fairness by comparing the statistical distribution of generated outputs across groups."@en .


###  http://fairness_ontology.com/EmbeddingBasedFairnessMetric
:EmbeddingBasedFairnessMetric rdf:type owl:Class ;
                              rdfs:subClassOf fmo:fairness_metric ;
                              rdfs:label "Embedding-based Fairness Metric"@en ;
                              skos:definition "A fairness metric that evaluates bias or fairness using representations derived from word or sentence embeddings."@en .


###  http://fairness_ontology.com/ExperimentalSetting
:ExperimentalSetting rdf:type owl:Class ;
                     rdfs:label "Experimental Setting"@en .


###  http://fairness_ontology.com/FairnessConcern
:FairnessConcern rdf:type owl:Class ;
                 rdfs:label "Fairness Concern"@en .


###  http://fairness_ontology.com/FairnessRegularization
:FairnessRegularization rdf:type owl:Class ;
                        rdfs:subClassOf :FullRetraining ;
                        rdfs:label "Fairness Regularization" .


###  http://fairness_ontology.com/FeatureLevelColumnIntervention
:FeatureLevelColumnIntervention rdf:type owl:Class ;
                                rdfs:subClassOf :DataResourcesMitigation ;
                                rdfs:label "Feature-level Column Intervention (A)" .


###  http://fairness_ontology.com/FeatureLevelRowReweightingResampling
:FeatureLevelRowReweightingResampling rdf:type owl:Class ;
                                      rdfs:subClassOf :DataResourcesMitigation ;
                                      rdfs:label "Feature-level Row Reweighting/Resampling (C)" .


###  http://fairness_ontology.com/FeatureReview
:FeatureReview rdf:type owl:Class ;
               rdfs:subClassOf :FeatureLevelColumnIntervention ;
               rdfs:label "Feature Review" .


###  http://fairness_ontology.com/FullFineTuning
:FullFineTuning rdf:type owl:Class ;
                rdfs:subClassOf :ModelAccessResourcesMitigation ,
                                :TrainingResourcesMitigation ;
                owl:disjointWith :PartialFineTuning ;
                rdfs:label "Full Fine-tuning - White-box Model (F)" .


###  http://fairness_ontology.com/FullParameterDebiasing
:FullParameterDebiasing rdf:type owl:Class ;
                        rdfs:subClassOf :FullFineTuning ;
                        rdfs:label "Full Parameter Debiasing" .


###  http://fairness_ontology.com/FullPruning
:FullPruning rdf:type owl:Class ;
             rdfs:subClassOf :FullFineTuning ;
             rdfs:comment "A mitigation technique that prunes an already trained model by setting to 0 some of its weights. The model is considered AS A WHOLE, by taking into account for pruning all its internal weights." ;
             rdfs:label "Full Pruning and Weight Masking" .


###  http://fairness_ontology.com/FullRetraining
:FullRetraining rdf:type owl:Class ;
                rdfs:subClassOf :TrainingResourcesMitigation ;
                rdfs:label "Full Retraining (E)" .


###  http://fairness_ontology.com/FullWeightRedistribution
:FullWeightRedistribution rdf:type owl:Class ;
                          rdfs:subClassOf :FullFineTuning ;
                          rdfs:comment "A mitigation technique that redistributes the weights of an already trained model, considering it AS A WHOLE, potentially redistributing all its weights." ;
                          rdfs:label "Full Weight Redistribution"@en .


###  http://fairness_ontology.com/GeneratedTextBasedFairnessMetric
:GeneratedTextBasedFairnessMetric rdf:type owl:Class ;
                                  rdfs:subClassOf fmo:fairness_metric ;
                                  rdfs:label "Generated Text-based Fairness Metric"@en ;
                                  skos:definition "A metric that measures fairness by analyzing or scoring generated text outputs."@en .


###  http://fairness_ontology.com/Governance
:Governance rdf:type owl:Class ;
            rdfs:subClassOf :AILifecycleStep ;
            rdfs:label "Governance" .


###  http://fairness_ontology.com/GovernanceOnly
:GovernanceOnly rdf:type owl:Class ;
                rdfs:subClassOf :HumanOversightMitigation ;
                rdfs:label "Governance-only Interventions (J)" .


###  http://fairness_ontology.com/GranularityMetric
:GranularityMetric rdf:type owl:Class ;
                   rdfs:subClassOf :RecommendationSystemMetric ;
                   rdfs:label "Granularity Metric"@en .


###  http://fairness_ontology.com/GreyBoxScores
:GreyBoxScores rdf:type owl:Class ;
               rdfs:subClassOf :ModelAccessResourcesMitigation ;
               rdfs:label "Scores and Probabilities Access - Grey-box Model (H)" .


